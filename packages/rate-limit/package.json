{
    "name": "@restyjs/rate-limit",
    "version": "3.0.0",
    "description": "Rate limiting middleware for Resty.js applications",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "license": "MIT",
    "sideEffects": false,
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint src",
        "typecheck": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest",
        "clean": "rm -rf .turbo node_modules dist"
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "peerDependencies": {
        "@restyjs/core": "^2.0.1"
    },
    "dependencies": {
        "ioredis": "^5.4.1"
    },
    "devDependencies": {
        "@restyjs/core": "workspace:*",
        "@types/express": "^5.0.0",
        "@types/node": "^22.10.2",
        "express": "^4.21.2",
        "typescript": "^5.7.2",
        "vitest": "^2.1.8"
    },
    "keywords": [
        "resty",
        "rate-limit",
        "throttle",
        "middleware"
    ]
}