{
    "name": "@restyjs/postman",
    "version": "1.1.0",
    "description": "Postman Collection v2.1 generator for Resty.js",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "license": "MIT",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup --watch",
        "lint": "eslint src",
        "typecheck": "tsc --noEmit",
        "test": "vitest run",
        "test:watch": "vitest",
        "clean": "rm -rf .turbo node_modules dist"
    },
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "peerDependencies": {
        "@restyjs/core": "^2.0.1"
    },
    "dependencies": {
        "postman-collection": "^5.0.0",
        "reflect-metadata": "^0.2.2"
    },
    "devDependencies": {
        "@restyjs/core": "workspace:*",
        "@types/postman-collection": "^3.5.0",
        "@types/node": "^22.10.2",
        "typescript": "^5.7.2",
        "tsup": "^8.3.5",
        "vitest": "^2.1.8"
    },
    "keywords": [
        "resty",
        "postman",
        "collection",
        "api",
        "testing"
    ]
}